KERNEL_UTIL_SRC_DIR := $(GET_LOCAL_DIR)
KERNEL_UTIL_OUT_DIR := $(KERNEL_OUT_DIR)/util
$(shell mkdir -p $(KERNEL_UTIL_OUT_DIR))

KERNEL_UTIL_O_FILES += \
	lists.o
	

KERNEL_UTIL_O_FILES := $(addprefix $(KERNEL_UTIL_OUT_DIR)/,$(KERNEL_UTIL_O_FILES))

KERNEL_O_FILES += $(KERNEL_UTIL_O_FILES)


$(KERNEL_UTIL_OUT_DIR)/%.o: $(KERNEL_UTIL_SRC_DIR)/%.c
	$(CC) -c $< $(GLOBAL_CFLAGS) $(KERNEL_CFLAGS) $(INCLUDES) -o $@
