LIBSTRING_ARCH_SRC_DIR := $(GET_LOCAL_DIR)
LIBSTRING_ARCH_OUT_DIR := $(LIBSTRING_OUT_DIR)/arch
$(shell mkdir -p $(LIBSTRING_ARCH_OUT_DIR))

LIBSTRING_O_FILES += $(LIBSTRING_ARCH_OUT_DIR)/memcpymov.o


$(LIBSTRING_ARCH_OUT_DIR)/%.o: $(LIBSTRING_ARCH_SRC_DIR)/%.S
	$(CC) -c $< $(GLOBAL_ASFLAGS) $(GLOBAL_CFLAGS) $(KERNEL_CFLAGS) $(INCLUDES) -o $@ 
